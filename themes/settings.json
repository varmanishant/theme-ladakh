  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#48d1cc"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#d996f6"
      }
    },
    {
      "scope": ["entity.name.type.class.python"],
      "settings": {
        "foreground": "#50c878"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ff3b66"
      }
    },
    {
      "scope": ["keyword.codetag.notation"],
      "settings": {
        "foreground": "#ff3b66"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#bcbcbc"
      }
    },
    {
      "scope": ["storage"],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "scope": ["string.quoted"],
      "settings": {
        "foreground": "#dac471"
      }
    },
    {
      "scope": ["string.quoted.docstring"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#50c878"
      }
    },
    {
      "scope": ["variable.parameter.function"],
      "settings": {
        "foreground": "#dac471"
      }
    }
  ]
