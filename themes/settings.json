  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5F6672"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#B57EDC"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "variable.language"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "scope": ["storage", "support.class"],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "scope": ["markup.inline.raw.string", "string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["support.type.property-name", "support.variable", "variable"],
      "settings": {
        "foreground": "#D4D7D9"
      }
    },
    {
      "scope": [
        "entity.other",
        "punctuation.definition.entity",
        "support.other"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["meta.brace", "punctuation"],
      "settings": {
        "foreground": "#A9B2C3"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": ["comment", "markup.italic", "punctuation.definition.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
