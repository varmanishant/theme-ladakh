    "tokenColors": [
        {
            "scope": ["comment"],
            "settings": {
                "foreground": "#888888"
            }
        },
        {
            "scope": ["constant"],
            "settings": {
                "foreground": "#66cdaa"
            }
        },
        {
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "#ae81ff"
            }
        },
        {
            "scope": ["entity.name.type.class"],
            "settings": {
                "foreground": "#50c878"
            }
        },
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#fa3f82"
            }
        },
        {
            "scope": ["keyword.codetag.notation"],
            "settings": {
                "foreground": "#fa3f82"
            }
        },
        {
            "scope": ["keyword.control.import"],
            "settings": {
                "foreground": "#fa3f82"
            }
        },
        {
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#dadada"
            }
        },
        {
            "scope": ["punctuation.definition.comment"],
            "settings": {
                "foreground": "#888888"
            }
        },
        {
            "scope": ["punctuation.definition.string"],
            "settings": {
                "foreground": "#cfc568"
            }
        },
        {
            "scope": ["storage"],
            "settings": {
                "foreground": "#61afef"
            }
        },
        {
            "scope": ["string.quoted"],
            "settings": {
                "foreground": "#cfc568"
            }
        },
        {
            "scope": ["support.type"],
            "settings": {
                "foreground": "#50c878"
            }
        },
        {
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#fda338"
            }
        }
    ]
